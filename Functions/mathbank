sum = num1+num2
mul = num1*num2
div = num1 / num2
sub = num1-num2
mod = num1 % num2
pwr = num1**num2
sqrt = num1**1/num2

print("SUM->", num1, "+", num2, "=", sum)
print("MULTIPLY->", num1, "x", num2, "=", mul)
print("DIVIDE->", num1, "/", num2, "=", div)
print("SUBTRACT->", num1, "-", num2, "=", sub)
print("MODULUS->", num1, "%", num2, "=", mod)
print("POWER->", num1, "**", num2, "=", pwr)
print("ROOT->", num1, "**1/", num2, "=", sqrt)